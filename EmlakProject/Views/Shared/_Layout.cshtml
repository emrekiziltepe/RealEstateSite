    @RenderSection("head", false)
</head>

<body>
    <nav class="navbar navbar-expand-lg navbar-dark ftco_navbar bg-dark ftco-navbar-light" id="ftco-navbar">
        <div class="container">
            <a class="navbar-brand" href="/Home/Index">EmLuck</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#ftco-nav" aria-controls="ftco-nav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="oi oi-menu"></span> Menu
            </button>

            <div class="collapse navbar-collapse" id="ftco-nav">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item active"><a href="/Home/Index" class="nav-link">AnaSayfa</a></li>
                    @if (User.IsInRole("1"))
                    {
                        <li class="nav-item"><a href="/Ilan/Index" class="nav-link">İlanlar</a></li>
                    }
                    <li class="nav-item"><a href="/Ilan/Satilik" class="nav-link">Satılık İlanlar</a></li>
                    <li class="nav-item"><a href="/Ilan/Kiralik" class="nav-link">Kiralık İlanlar</a></li>
                    <li class="nav-item"><a href="/Ilan/AdresEkle" class="nav-link">İlan Ekle</a></li>
                    @*<li class="nav-item"><a href="/Login/Login" class="nav-link">Üye Girişi</a></li>*@
                    @if (@HttpContext.Current.User.Identity.Name == "")
                    {
                        <li class="nav-item"><a href="/Login/Login" class="nav-link">Üye Girişi</a></li>
                    }
                    else
                    {
                        <li class="nav-item dropdown">
                            <a href="#" class="nav-link dropdown-toggle">Hoşgeldiniz, @HttpContext.Current.User.Identity.Name</a>
                            <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                                <a class="dropdown-item" href="/Login/Profil">Profil</a>
                                @if (User.IsInRole("1"))
                                {
                                    <a class="dropdown-item" href="/Ilan/Onay">Onay Bekleyen İlanlar</a>
                                }
                                else
                                {
                                    <a class="dropdown-item" href="/Ilan/Ilanlarim">İlanlarım</a>
                                }
                                <a class="dropdown-item" href="/Login/LogOut">Çıkış Yap</a>
                            </div>
                        </li>
                    }
                </ul>
            </div>
        </div>
    </nav>

    @RenderBody()

